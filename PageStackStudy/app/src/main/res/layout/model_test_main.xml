    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_vertical">

    <Button
        android:id="@+id/tv_test_icallback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="回调"/>

    <Button
        android:id="@+id/tv_test_observer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="观察者之微信公众号"/>

    <Button
        android:id="@+id/tv_test_wolf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="观察者之狼群开会"/>

</LinearLayout>